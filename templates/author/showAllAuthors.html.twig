<table class="table table-striped" border="1">
    <th>ID</th>
    <th>Username</th>
    <th>Email</th>
    <th>Actions</th>


    {% for author in authors %}
    <tr>
        <td>{{author.id}}</td>
        <td>{{author.username}}</td>
        <td>{{author.email}}</td>
        <td> <a href="{{path('deleteAuthor',{'id': author.id})}}">Delete</a> </td>
        <td> <a href="{{path('editAuthor',{'id': author.id})}}">Edit</a> </td>
    </tr>
    {% endfor %}
</table>