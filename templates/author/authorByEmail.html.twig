<table class="table" border="2">
    <th>ID</th>
    <th>Username</th>
    <th>Email</th>
    <th>Actions</th>

    {% for author in authors %}
        <tr>
            <td>{{ author.id }}</td>
            <td>{{ author.username }}</td>
            <td>{{ author.email }}</td>
        </tr>
    {% endfor %}

</table>